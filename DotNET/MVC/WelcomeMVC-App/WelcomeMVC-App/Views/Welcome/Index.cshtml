
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        <h1>Index View of Welcome Controller</h1>

        @for (var i = 1; i <= 6; i++)
        {
            <h1>Hello, @i</h1>
        }

        @for (var i = 1; i <= 6; i++)
        {
            @Html.Raw("<h" + i + ">Swabhav Techlabs</h" + i + ">");
            <br />
        }
    </div>
</body>
</html>
